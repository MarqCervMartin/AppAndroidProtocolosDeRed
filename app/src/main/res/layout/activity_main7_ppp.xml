<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main7ActivityPPP">

    <TextView
        android:id="@+id/b1"
        android:layout_width="58dp"
        android:layout_height="19dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="96dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/direc"
        android:layout_width="58dp"
        android:layout_height="19dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="96dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/b1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cntl"
        android:layout_width="55dp"
        android:layout_height="17dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="96dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/direc"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/prot"
        android:layout_width="58dp"
        android:layout_height="19dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="96dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/cntl"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cr"
        android:layout_width="58dp"
        android:layout_height="19dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="76dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tex"
        app:layout_constraintTop_toBottomOf="@+id/cntl" />

    <TextView
        android:id="@+id/tex"
        android:layout_width="76dp"
        android:layout_height="19dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="80dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b1" />

    <TextView
        android:id="@+id/b2"
        android:layout_width="58dp"
        android:layout_height="20dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="72dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/cr"
        app:layout_constraintTop_toBottomOf="@+id/prot" />

</android.support.constraint.ConstraintLayout>